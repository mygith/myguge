<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>AOL Search</os:ShortName>
<os:Description>AOL Search</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://www.aolsearch.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.aolsearch.com/search" resultDomain="aolsearch.com">
  <os:Param name="s_it" value="searchbox.webhome"/>
  <os:Param name="v_t" value="na"/>
  <os:Param name="q" value="{searchTerms}"/>
</os:Url>
</SearchPlugin>